
integrator               = md        
dt                       = 0.002     ; 2 fs
nsteps                   = 50000     ; 100 ps

nstenergy                = 200
nstlog                   = 2000
nstxout-compressed       = 10000

gen-vel                  = yes
gen-temp                 = 298.15

constraint-algorithm     = lincs
constraints              = h-bonds

cutoff-scheme            = Verlet

coulombtype              = PME
rcoulomb                 = 1.0

vdwtype                  = Cut-off
rvdw                     = 1.0
DispCorr                 = EnerPres

tcoupl                   = Nose-Hoover
tc-grps                  = System
tau-t                    = 2.0
ref-t                    = 298.15
nhchainlength            = 1

pull                     = umbrella 
pull-geometry            = distance
pull-start               = no
pull-ngroups             = 2
pull-coord1-groups       = 1 2
pull-group1-name         = CA
pull-group2-name         = CB
pull-coord1-k            = 5000.0
pull-coord1-init         = WINDOW
pull-coord1-rate         = 0.0
